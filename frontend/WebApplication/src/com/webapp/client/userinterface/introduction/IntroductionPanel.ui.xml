<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='css'
		type='com.webapp.client.userinterface.resources.Resources' />
	<g:HTMLPanel>
		<g:VerticalPanel styleName="{css.introduction.main}"
			ui:field="main">
			<g:Label styleName="{css.introduction.header}">CS 183 Web Application</g:Label>
			<g:HTMLPanel>
				<table>
					<tr>
						<td>
							<g:Label styleName="{css.introduction.label}">Number of reads from MySQL: </g:Label>
						</td>
						<td>
							<g:TextBox styleName="{css.introduction.boxes}"
								ui:field="reads" />
						</td>
					</tr>
					<tr>
						<td>
							<g:Label styleName="{css.introduction.label}">Number of writes from MySQL: </g:Label>
						</td>
						<td>
							<g:TextBox styleName="{css.introduction.boxes}"
								ui:field="writes" />
						</td>
					</tr>
					<tr>
						<td>
							<g:Label styleName="{css.introduction.label}">Result Destination (file path): </g:Label>
						</td>
						<td>
							<g:TextBox styleName="{css.introduction.boxes}"
								ui:field="filePath" />
						</td>
					</tr>
				</table>
			</g:HTMLPanel>
			<g:Button ui:field="submit">Continue</g:Button>
			<g:Label styleName="{css.introduction.version}" ui:field="version" />
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 