<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:with field='css'
		type='com.webapp.client.userinterface.resources.Resources' />
	<g:HTMLPanel>
		<g:VerticalPanel styleName="{css.mainpage.main}"
			ui:field="main">
			<g:HorizontalPanel>
				<g:Label styleName="{css.mainpage.header}">Status: </g:Label>
				<g:Label ui:field="header" styleName="{css.mainpage.header}" />
			</g:HorizontalPanel>

			<g:Label ui:field="results" styleName="{css.mainpage.results}" />
			<g:Label ui:field="pathInformation" styleName="{css.mainpage.results}" />
		</g:VerticalPanel>
		<g:Label styleName="{css.mainpage.version}">Version 0.1</g:Label>
	</g:HTMLPanel>
</ui:UiBinder> 