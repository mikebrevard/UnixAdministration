
global
  log 127.0.0.1 local2 notice
  maxconn 2000
  user haproxy
  group haproxy

defaults
  log     global
  mode    tcp
  option  tcplog
  option  dontlognull
  retries 3
  option  redispatch
  timeout connect 5000
  timeout client 10000
  timeout server 10000

listen appname  *:8081
  mode tcp
  balance roundrobin
